{
  "name": "Robo-Kit",
  "version": "1.0.0",
  "private": true,
  "description": "Robo-Kit",
  "author": "scalecube.io <scalecube.io@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/scalecube/robokit.git",
  "homepage": "https://github.com/scalecube/robolit",
  "bugs": "https://github.com/scalecube/robokit/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "./node_modules/.bin/nodemon",
    "robokit": "node ./robokit.js",
    "robokit-k8s": "node ./robokit-k8s.js",
    "start": "./node_modules/.bin/probot run ./index.js",
    "lint": "./node_modules/.bin/standard --fix",
    "build": "browserify ./app/views/main.js -o ./app/views/bundle.js --debug -t [ babelify --presets [ @babel/preset-env @babel/preset-react ]  ]",
    "watch": "watchify ./app/views/main.js -o ./app/views/bundle.js --debug -t [ babelify --presets [ @babel/preset-env @babel/preset-react ]  ]"
  },
  "dependencies": {
    "@octokit/auth": "^3.0.3",
    "api-key-auth": "^0.2.3",
    "auth-routes": "0.0.1",
    "axios": "^1.7.9",
    "axios-cookiejar-support": "^5.0.3",
    "body-parser": "^1.20.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "cron": "^3.3.1",
    "express-access-token": "^1.1.8",
    "express-jwt": "^8.5.1",
    "express-jwt-authz": "^2.4.1",
    "github-oauth": "^0.2.2",
    "jquery": "^3.7.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.12.0",
    "mui-datatables": "^4.3.0",
    "node-vault": "^0.10.2",
    "node-vault-client": "^1.0.1",
    "nodejs-websocket": "^1.7.2",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-github-token": "^3.0.0",
    "plotly.js": "^2.35.2",
    "probot": "^13.4.1",
    "probot-scheduler": "^2.0.0-beta.1",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.0.0",
    "react-plotly.js": "^2.6.0",
    "react-tools": "^0.13.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rxjs": "^7.8.1",
    "rxjs-compat": "^6.6.7",
    "serve-static": "1.16.2",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^10.0.0",
    "smee-client": "^2.0.4",
    "nock": "^13.5.6",
    "nodemon": "^3.1.7",
    "standard": "^17.1.2"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
